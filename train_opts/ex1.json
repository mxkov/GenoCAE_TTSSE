{
  "learning_rate": 0.00032,
  "batch_size": 10,
  "noise_std": 0.0032,
  "n_samples": -1,
  "loss": {
    "module": "tf.keras.losses",
    "class": "MeanSquaredError"
  },
  "regularizer": {
    "reg_factor": 1e-07,
    "module": "tf.keras.regularizers",
    "class": "l2"
  },
  "lr_scheme": {
    "module": "tf.keras.optimizers.schedules",
    "class": "ExponentialDecay",
    "args": {
      "decay_rate": 0.99,
      "staircase": false,
      "decay_steps": 1
    }}

}


